{
    "compilerOptions": {
        "outDir": "./dist",
        "sourceMap": true,
        "lib": ["dom", "dom.iterable", "ES2020"],
        "noImplicitAny": true,
        "removeComments": true,
        "module": "commonjs",
        "target": "ES2020",
        "jsx": "react",
        "allowJs": false,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    },
    "exclude": ["node_modules"]
}
